{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "jest/globals": true 
    },
    "plugins": ["jest"],
    "extends":["eslint:recommended","plugin:jest/recommended","plugin:prettier/recommended"],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
    "no-constant-condition": ["error", { "checkLoops": false }]
    }
}
